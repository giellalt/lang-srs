! 0-PFV-STAT
!
! 0-PFV-STAT and 0-IPFV-STAT share the exact same inflectional markers, and
! differ only in aspect (imperfective vs. perfective).  They are also both
! almost identical in form to 0-IPFV, differing only in some 3PL and 4SG forms
! (see comments on 0-IPFV-STAT above).  We therefore reuse those 0-IPFV TAMA
! chunk forms, relying on flag diacritics matching this TAMA class to produce
! distinctive 3PL and 4SG forms where needed.
!
LEXICON 0-PFV-STAT
@U.ASPECT.PFV@@U.TAMA.%0-STAT@               0-IPFV-AFF ;


!
! ni-PFV
!
LEXICON ni-PFV
@U.ASPECT.PFV@@U.TAMA.ni@                    ni-PFV-AFF ;

LEXICON ni-PFV-AFF
@U.VV.%0@@D.PREFIX@                          ni-PFV-AFF-NoPrefix ;
@U.VV.%0@@R.PREFIX.INNER@                    ni-PFV-AFF-InnerPrefix ;
@U.VV.%0@@R.PREFIX.MIDDLE@                   ni-PFV-AFF-MiddlePrefix ;
@U.VV.%0@@R.PREFIX.OUTER@                    ni-PFV-AFF-OuterPrefix ;

@U.VV.S@@D.PREFIX@                           nis-PFV-AFF-NoPrefix ;
@U.VV.S@@R.PREFIX.INNER@                     nis-PFV-AFF-InnerPrefix ;
@U.VV.S@@R.PREFIX.MIDDLE@                    nis-PFV-AFF-MiddlePrefix ;
@U.VV.S@@R.PREFIX.OUTER@                     nis-PFV-AFF-OuterPrefix ;

@U.VV.I@@D.PREFIX@                           nii-PFV-AFF-NoPrefix ;
@U.VV.I@@R.PREFIX.INNER@                     nii-PFV-AFF-InnerPrefix ;
@U.VV.I@@R.PREFIX.MIDDLE@                    nii-PFV-AFF-MiddlePrefix ;
@U.VV.I@@R.PREFIX.OUTER@                     nii-PFV-AFF-OuterPrefix ;

LEXICON ni-PFV-AFF-NoPrefix
! FIXME: no documentation for this form; dummy values below
@U.SUBJECTPERSON.1@@U.SUBJECTNUMBER.SG@nis   #;
@U.SUBJECTPERSON.2@@U.SUBJECTNUMBER.SG@ní    #;
@U.SUBJECTPERSON.3@@U.SUBJECTNUMBER.SG@ní    #;
@U.SUBJECTPERSON.1@@U.SUBJECTNUMBER.PL@@U.DIALECT.BIGPLUME@naà  #;
@U.SUBJECTPERSON.1@@U.SUBJECTNUMBER.PL@@U.DIALECT.BIGCROW@niì   #;
@U.SUBJECTPERSON.1@@U.SUBJECTNUMBER.PL@@U.DIALECT.CROWCHIEF@niì #;
@U.SUBJECTPERSON.2@@U.SUBJECTNUMBER.PL@nas2  #;
@R.SUBJECTPERSON.3@@R.SUBJECTNUMBER.PL@ní    #;
@R.SUBJECTPERSON.4@@R.SUBJECTNUMBER.SG@ní    #;

LEXICON ni-PFV-AFF-InnerPrefix
@U.SUBJECTPERSON.1@@U.SUBJECTNUMBER.SG@^Pnis  #; ! FLOATING L-TONE (M/L -> L)
@U.SUBJECTPERSON.2@@U.SUBJECTNUMBER.SG@^Pní   #; ! FLOATING L-TONE
@U.SUBJECTPERSON.3@@U.SUBJECTNUMBER.SG@^Pní   #; ! FLOATING L-TONE
@U.SUBJECTPERSON.1@@U.SUBJECTNUMBER.PL@@U.DIALECT.BIGPLUME@^Pnaà  #; ! FLOATING L-TNE
@U.SUBJECTPERSON.1@@U.SUBJECTNUMBER.PL@@U.DIALECT.BIGCROW@^Pniì   #; ! FLOATING L-TONE
@U.SUBJECTPERSON.1@@U.SUBJECTNUMBER.PL@@U.DIALECT.CROWCHIEF@^Pniì  #; ! FLOATING L-NE
@U.SUBJECTPERSON.2@@U.SUBJECTNUMBER.PL@^Pnas2 #; ! FLOATING L-TONE
@R.SUBJECTPERSON.3@@R.SUBJECTNUMBER.PL@^Pní   #; ! FLOATING L-TONE
@R.SUBJECTPERSON.4@@R.SUBJECTNUMBER.SG@^Pní   #; ! FLOATING L-TONE

LEXICON ni-PFV-AFF-MiddlePrefix
! Throughout the ni-PFV paradigm, middle-prefix forms containing a direct
! object prefix appear with floating L tone (e.g., násigìnístín "they both
! took me home", not *násiginístín).
!
! By contrast, middle-prefix forms without a direct object prefix (e.g.,
! with gi– 3PL.SBJ and ts'i- 4SG.SBJ) do not appear with low tone (e.g.,
! nàts'iníʔón "someone put it down (solid object)", not nàts'ìníʔón).  We
! model this here by checking for the presence of object marking on the
! verb via the flag diacritic DIRECTOBJECTPRESENT.
@R.DIRECTOBJECTPRESENT.ON@                   ni-PFV-AFF-InnerPrefix;
@D.DIRECTOBJECTPRESENT@                      ni-PFV-AFF-OuterPrefix;

LEXICON ni-PFV-AFF-OuterPrefix
@U.SUBJECTPERSON.1@@U.SUBJECTNUMBER.SG@nis   #;
@U.SUBJECTPERSON.2@@U.SUBJECTNUMBER.SG@ní    #;
@U.SUBJECTPERSON.3@@U.SUBJECTNUMBER.SG@ní    #;
@U.SUBJECTPERSON.1@@U.SUBJECTNUMBER.PL@@U.DIALECT.BIGPLUME@naà  #;
@U.SUBJECTPERSON.1@@U.SUBJECTNUMBER.PL@@U.DIALECT.BIGCROW@niì   #;
@U.SUBJECTPERSON.1@@U.SUBJECTNUMBER.PL@@U.DIALECT.CROWCHIEF@niì #;
@U.SUBJECTPERSON.2@@U.SUBJECTNUMBER.PL@nas2  #;
@R.SUBJECTPERSON.3@@R.SUBJECTNUMBER.PL@ní    #;
@R.SUBJECTPERSON.4@@R.SUBJECTNUMBER.SG@ní    #;


LEXICON nis-PFV-AFF-NoPrefix
! FIXME: no documentation for this form; dummy values below
@U.SUBJECTPERSON.1@@U.SUBJECTNUMBER.SG@nis   #;
@U.SUBJECTPERSON.2@@U.SUBJECTNUMBER.SG@nís   #;
@U.SUBJECTPERSON.3@@U.SUBJECTNUMBER.SG@nis   #;
@U.SUBJECTPERSON.1@@U.SUBJECTNUMBER.PL@@U.DIALECT.BIGPLUME@naà  #;
@U.SUBJECTPERSON.1@@U.SUBJECTNUMBER.PL@@U.DIALECT.BIGCROW@niì   #;
@U.SUBJECTPERSON.1@@U.SUBJECTNUMBER.PL@@U.DIALECT.CROWCHIEF@niì #;
@U.SUBJECTPERSON.2@@U.SUBJECTNUMBER.PL@nas2  #;
@R.SUBJECTPERSON.3@@R.SUBJECTNUMBER.PL@nís   #;
@R.SUBJECTPERSON.4@@R.SUBJECTNUMBER.SG@nís   #;

LEXICON nis-PFV-AFF-InnerPrefix
@U.SUBJECTPERSON.1@@U.SUBJECTNUMBER.SG@^Pnis  #; ! FLOATING L-TONE
@U.SUBJECTPERSON.2@@U.SUBJECTNUMBER.SG@^Pnís  #; ! FLOATING L-TONE
@U.SUBJECTPERSON.3@@U.SUBJECTNUMBER.SG@^Pnís  #; ! FLOATING L-TONE
@U.SUBJECTPERSON.1@@U.SUBJECTNUMBER.PL@@U.DIALECT.BIGPLUME@^Pnaà  #; ! FLOATING L-TNE
@U.SUBJECTPERSON.1@@U.SUBJECTNUMBER.PL@@U.DIALECT.BIGCROW@^Pniì   #; ! FLOATING L-TONE
@U.SUBJECTPERSON.1@@U.SUBJECTNUMBER.PL@@U.DIALECT.CROWCHIEF@^Pniì #; ! FLOATING L-NE
@U.SUBJECTPERSON.2@@U.SUBJECTNUMBER.PL@^Pnas2 #; ! FLOATING L-TONE
@R.SUBJECTPERSON.3@@R.SUBJECTNUMBER.PL@^Pnís  #; ! FLOATING L-TONE
@R.SUBJECTPERSON.4@@R.SUBJECTNUMBER.SG@^Pnís  #; ! FLOATING L-TONE

LEXICON nis-PFV-AFF-MiddlePrefix
@R.DIRECTOBJECTPRESENT.ON@                   nis-PFV-AFF-InnerPrefix;
@D.DIRECTOBJECTPRESENT@                      nis-PFV-AFF-OuterPrefix;

LEXICON nis-PFV-AFF-OuterPrefix
@U.SUBJECTPERSON.1@@U.SUBJECTNUMBER.SG@nis   #;
@U.SUBJECTPERSON.2@@U.SUBJECTNUMBER.SG@nís   #;
@U.SUBJECTPERSON.3@@U.SUBJECTNUMBER.SG@nís   #;
@U.SUBJECTPERSON.1@@U.SUBJECTNUMBER.PL@@U.DIALECT.BIGPLUME@naà  #;
@U.SUBJECTPERSON.1@@U.SUBJECTNUMBER.PL@@U.DIALECT.BIGCROW@niì   #;
@U.SUBJECTPERSON.1@@U.SUBJECTNUMBER.PL@@U.DIALECT.CROWCHIEF@niì #;
@U.SUBJECTPERSON.2@@U.SUBJECTNUMBER.PL@nas2  #;
@R.SUBJECTPERSON.3@@R.SUBJECTNUMBER.PL@nís   #;
@R.SUBJECTPERSON.4@@R.SUBJECTNUMBER.SG@nís   #;


LEXICON nii-PFV-AFF-NoPrefix
! FIXME: no documentation for this form; dummy values below
@U.SUBJECTPERSON.1@@U.SUBJECTNUMBER.SG@nis   #;
@U.SUBJECTPERSON.2@@U.SUBJECTNUMBER.SG@níí   #;
@U.SUBJECTPERSON.3@@U.SUBJECTNUMBER.SG@nii   #;
@U.SUBJECTPERSON.1@@U.SUBJECTNUMBER.PL@@U.DIALECT.BIGPLUME@naà  #;
@U.SUBJECTPERSON.1@@U.SUBJECTNUMBER.PL@@U.DIALECT.BIGCROW@niì   #;
@U.SUBJECTPERSON.1@@U.SUBJECTNUMBER.PL@@U.DIALECT.CROWCHIEF@niì #;
@U.SUBJECTPERSON.2@@U.SUBJECTNUMBER.PL@nas2  #;
@R.SUBJECTPERSON.3@@R.SUBJECTNUMBER.PL@níí   #;
@R.SUBJECTPERSON.4@@R.SUBJECTNUMBER.SG@níí   #;

LEXICON nii-PFV-AFF-InnerPrefix
! FIXME: no documentation for this form; dummy values below
@U.SUBJECTPERSON.1@@U.SUBJECTNUMBER.SG@^Pnis  #; ! FLOATING L-TONE
@U.SUBJECTPERSON.2@@U.SUBJECTNUMBER.SG@^Pníí  #; ! FLOATING L-TONE
@U.SUBJECTPERSON.3@@U.SUBJECTNUMBER.SG@^Pníí  #; ! FLOATING L-TONE
@U.SUBJECTPERSON.1@@U.SUBJECTNUMBER.PL@@U.DIALECT.BIGPLUME@^Pnaà  #; ! FLOATING L-TNE
@U.SUBJECTPERSON.1@@U.SUBJECTNUMBER.PL@@U.DIALECT.BIGCROW@^Pniì   #; ! FLOATING L-TONE
@U.SUBJECTPERSON.1@@U.SUBJECTNUMBER.PL@@U.DIALECT.CROWCHIEF@^Pniì #; ! FLOATING L-NE
@U.SUBJECTPERSON.2@@U.SUBJECTNUMBER.PL@^Pnas2 #; ! FLOATING L-TONE
@R.SUBJECTPERSON.3@@R.SUBJECTNUMBER.PL@^Pníí  #; ! FLOATING L-TONE
@R.SUBJECTPERSON.4@@R.SUBJECTNUMBER.SG@^Pníí  #; ! FLOATING L-TONE

LEXICON nii-PFV-AFF-MiddlePrefix
@R.DIRECTOBJECTPRESENT.ON@                   nii-PFV-AFF-InnerPrefix;
@D.DIRECTOBJECTPRESENT@                      nii-PFV-AFF-OuterPrefix;

LEXICON nii-PFV-AFF-OuterPrefix
@U.SUBJECTPERSON.1@@U.SUBJECTNUMBER.SG@nis   #;
@U.SUBJECTPERSON.2@@U.SUBJECTNUMBER.SG@níí   #;
@U.SUBJECTPERSON.3@@U.SUBJECTNUMBER.SG@ii    #;
@U.SUBJECTPERSON.1@@U.SUBJECTNUMBER.PL@@U.DIALECT.BIGPLUME@naà  #;
@U.SUBJECTPERSON.1@@U.SUBJECTNUMBER.PL@@U.DIALECT.BIGCROW@niì   #;
@U.SUBJECTPERSON.1@@U.SUBJECTNUMBER.PL@@U.DIALECT.CROWCHIEF@niì #;
@U.SUBJECTPERSON.2@@U.SUBJECTNUMBER.PL@nas2  #;
@R.SUBJECTPERSON.3@@R.SUBJECTNUMBER.PL@íí    #;
@R.SUBJECTPERSON.4@@R.SUBJECTNUMBER.SG@íí    #;


!
! si-IPFV
!
! si-IPFV and si-PFV share the exact same inflectional markers, and differ
! only in aspect (imperfective vs. perfective).  We can therefore tag all
! si-IPFV forms passing through this lexicon as imperfective, then reuse
! the TAMA chunks defined in si-AFF below.
!
LEXICON si-IPFV
@U.ASPECT.IPFV@@U.TAMA.si@                   si-AFF ;


!
! si-PFV
!
! si-IPFV and si-PFV share the exact same inflectional markers, and differ
! only in aspect (imperfective vs. perfective).  We can therefore tag all
! si-PFV forms passing through this lexicon as perfective, then reuse the
! TAMA chunks defined in si-AFF below.
!
LEXICON si-PFV
@U.ASPECT.PFV@@U.TAMA.si@                    si-AFF ;

!
! si-AFF
!
! Note that 3PL gi- and 4 ts'i- (verb_middle_affixes) don't receive low tone
! like other inner prefixes in this paradigm (e.g., gisjón "they are old",
! not *gìsjón).
!
! To make sure that 3PL and 4 forms receive the right tone, we make sure that
! forms involving these two prefixes (and no other inner prefixes) end up
! heading through the -MiddlePrefix continuation lexica below, which have
! 3PL and 4 inflectional chunks without the low tone.
!
! TODO: test this with a few transitive verbs and make sure that the tone
! is coming out properly in all cases! (need to be sure that transitive
! verb inflection, which is also a kind of middle prefixation, is also coming
! out with the right tone assignments)
!

LEXICON si-AFF

@U.VV.%0@@D.PREFIX@                          si-AFF-NoPrefix ;
@U.VV.%0@@R.PREFIX.INNER@                    si-AFF-InnerPrefix ;
@U.VV.%0@@R.PREFIX.MIDDLE@                   si-AFF-MiddlePrefix ;
@U.VV.%0@@R.PREFIX.OUTER@                    si-AFF-OuterPrefix ;

@U.VV.S@@D.PREFIX@                           sis-AFF-NoPrefix ;
@U.VV.S@@R.PREFIX.INNER@                     sis-AFF-InnerPrefix ;
@U.VV.S@@R.PREFIX.MIDDLE@                    sis-AFF-MiddlePrefix ;
@U.VV.S@@R.PREFIX.OUTER@                     sis-AFF-OuterPrefix ;

@U.VV.I@@D.PREFIX@                           sii-AFF-NoPrefix ;
@U.VV.I@@R.PREFIX.INNER@                     sii-AFF-InnerPrefix ;
@U.VV.I@@R.PREFIX.MIDDLE@                    sii-AFF-MiddlePrefix ;
@U.VV.I@@R.PREFIX.OUTER@                     sii-AFF-OuterPrefix ;


LEXICON si-AFF-NoPrefix
@U.SUBJECTPERSON.1@@U.SUBJECTNUMBER.SG@sis   #;
@U.SUBJECTPERSON.2@@U.SUBJECTNUMBER.SG@sí    #;
@U.SUBJECTPERSON.3@@U.SUBJECTNUMBER.SG@si    #;
@U.SUBJECTPERSON.1@@U.SUBJECTNUMBER.PL@@U.DIALECT.BIGPLUME@saà  #;
@U.SUBJECTPERSON.1@@U.SUBJECTNUMBER.PL@@U.DIALECT.BIGCROW@siì   #;
@U.SUBJECTPERSON.1@@U.SUBJECTNUMBER.PL@@U.DIALECT.CROWCHIEF@siì #;
@U.SUBJECTPERSON.2@@U.SUBJECTNUMBER.PL@sas2  #;
@R.SUBJECTPERSON.3@@R.SUBJECTNUMBER.PL@s3    #;  
@R.SUBJECTPERSON.4@@R.SUBJECTNUMBER.SG@s3    #; 

LEXICON si-AFF-InnerPrefix
@U.SUBJECTPERSON.1@@U.SUBJECTNUMBER.SG@ìsis  #; ! FLOATING L-TONE
! When an inner prefix is present, 1SG forms also appear as Cas (e.g.,
! dìsists'ón ~ dasts'ón "I heard (it)", zìsisyín ~ zasyín "I killed (it)".
@U.SUBJECTPERSON.1@@U.SUBJECTNUMBER.SG@as    #;
@U.SUBJECTPERSON.2@@U.SUBJECTNUMBER.SG@^Psí   #; ! FLOATING L-TONE
@U.SUBJECTPERSON.3@@U.SUBJECTNUMBER.SG@ìs3   #;
@U.SUBJECTPERSON.1@@U.SUBJECTNUMBER.PL@@U.DIALECT.BIGPLUME@^Psaà  #; ! FLOATING L-TNE
@U.SUBJECTPERSON.1@@U.SUBJECTNUMBER.PL@@U.DIALECT.BIGCROW@^Psiì  #; ! FLOATING L-TONE
@U.SUBJECTPERSON.1@@U.SUBJECTNUMBER.PL@@U.DIALECT.CROWCHIEF@^Psiì  #; ! FLOATING L-NE
@U.SUBJECTPERSON.2@@U.SUBJECTNUMBER.PL@^Psas2 #; ! FLOATING L-TONE
@R.SUBJECTPERSON.3@@R.SUBJECTNUMBER.PL@ìs3   #;
@R.SUBJECTPERSON.4@@R.SUBJECTNUMBER.SG@ìs3   #;

LEXICON si-AFF-MiddlePrefix
@U.SUBJECTPERSON.1@@U.SUBJECTNUMBER.SG@^Psis  #; ! FLOATING L-TONE
@U.SUBJECTPERSON.2@@U.SUBJECTNUMBER.SG@^Psí   #; ! FLOATING L-TONE
@U.SUBJECTPERSON.3@@U.SUBJECTNUMBER.SG@ìs3   #;
@U.SUBJECTPERSON.1@@U.SUBJECTNUMBER.PL@@U.DIALECT.BIGPLUME@^Psaà  #; ! FLOATING L-TNE
@U.SUBJECTPERSON.1@@U.SUBJECTNUMBER.PL@@U.DIALECT.BIGCROW@^Psiì  #; ! FLOATING L-TONE
@U.SUBJECTPERSON.1@@U.SUBJECTNUMBER.PL@@U.DIALECT.CROWCHIEF@^Psiì  #; ! FLOATING L-NE
@U.SUBJECTPERSON.2@@U.SUBJECTNUMBER.PL@^Psas2 #; ! FLOATING L-TONE
! When a direct object marker is present on the verb, 3PL and 4SG subjects
! appear with a low tone (e.g., tásigìschíízh "they both painted me", with
! gìs-, not *gis-).  When there is no direct object marker, 3PL and 4SG
! subjects appear with unmarked (mid) tone (e.g., O tágischíízh "they both
! painted O", with gis-, not *gìs-).
@R.DIRECTOBJECTPRESENT.ON@@R.SUBJECTPERSON.3@@R.SUBJECTNUMBER.PL@^Ps3    #;
@R.DIRECTOBJECTPRESENT.ON@@R.SUBJECTPERSON.4@@R.SUBJECTNUMBER.SG@^Ps3    #;
@D.DIRECTOBJECTPRESENT@@R.SUBJECTPERSON.3@@R.SUBJECTNUMBER.PL@s3    #;
@D.DIRECTOBJECTPRESENT@@R.SUBJECTPERSON.4@@R.SUBJECTNUMBER.SG@s3    #;

LEXICON si-AFF-OuterPrefix
@U.SUBJECTPERSON.1@@U.SUBJECTNUMBER.SG@sis   #;
@U.SUBJECTPERSON.2@@U.SUBJECTNUMBER.SG@sí    #;
@U.SUBJECTPERSON.3@@U.SUBJECTNUMBER.SG@si    #;
@U.SUBJECTPERSON.1@@U.SUBJECTNUMBER.PL@@U.DIALECT.BIGPLUME@saà  #;
@U.SUBJECTPERSON.1@@U.SUBJECTNUMBER.PL@@U.DIALECT.BIGCROW@siì   #;
@U.SUBJECTPERSON.1@@U.SUBJECTNUMBER.PL@@U.DIALECT.CROWCHIEF@siì #;
@U.SUBJECTPERSON.2@@U.SUBJECTNUMBER.PL@sas2  #;
@R.SUBJECTPERSON.3@@R.SUBJECTNUMBER.PL@s3    #;
@R.SUBJECTPERSON.4@@R.SUBJECTNUMBER.SG@s3    #;


LEXICON sis-AFF-NoPrefix
@U.SUBJECTPERSON.1@@U.SUBJECTNUMBER.SG@sis   #;
! FIXME: No documentation of 2SG forms of sis-PFV verbs with /l/-initial stems
! exists, making it impossible to know whether these trigger dissimilation 
! (and should be represented here as <s3>) or not (and should be represented
! here as <s>).
@U.SUBJECTPERSON.2@@U.SUBJECTNUMBER.SG@sís   #;
@U.SUBJECTPERSON.3@@U.SUBJECTNUMBER.SG@sis3  #;
@U.SUBJECTPERSON.1@@U.SUBJECTNUMBER.PL@@U.DIALECT.BIGPLUME@saà  #;
@U.SUBJECTPERSON.1@@U.SUBJECTNUMBER.PL@@U.DIALECT.BIGCROW@siì   #;
@U.SUBJECTPERSON.1@@U.SUBJECTNUMBER.PL@@U.DIALECT.CROWCHIEF@siì #;
@U.SUBJECTPERSON.2@@U.SUBJECTNUMBER.PL@sas2  #;
@R.SUBJECTPERSON.3@@R.SUBJECTNUMBER.PL@s3    #;
@R.SUBJECTPERSON.4@@R.SUBJECTNUMBER.SG@s3    #;

LEXICON sis-AFF-InnerPrefix
@U.SUBJECTPERSON.1@@U.SUBJECTNUMBER.SG@^Psis  #; ! FLOATING L-TONE (M/L -> L)
! When an inner prefix is present, 1SG forms also appear as Cas (e.g.,
! dìsists'ón ~ dasts'ón "I heard (it)", zìsisyín ~ zasyín "I killed (it)".
@U.SUBJECTPERSON.1@@U.SUBJECTNUMBER.SG@as    #;
! FIXME: No documentation of 2SG forms of sis-PFV verbs with /l/-initial stems.
@U.SUBJECTPERSON.2@@U.SUBJECTNUMBER.SG@^Psís  #; ! FLOATING L-TONE (M/L -> L)
@U.SUBJECTPERSON.3@@U.SUBJECTNUMBER.SG@ìs3   #;
@U.SUBJECTPERSON.1@@U.SUBJECTNUMBER.PL@@U.DIALECT.BIGPLUME@^Psaà  #; ! FLOATING L-TONE (M/L ->L)
@U.SUBJECTPERSON.1@@U.SUBJECTNUMBER.PL@@U.DIALECT.BIGCROW@^Psiì  #; ! FLOATING L-TONE (M/L -> L)
@U.SUBJECTPERSON.1@@U.SUBJECTNUMBER.PL@@U.DIALECT.CROWCHIEF@^Psiì  #; ! FLOATING L-TONE (M/L -L)
@U.SUBJECTPERSON.2@@U.SUBJECTNUMBER.PL@^Psas2 #; ! FLOATING L-TONE (M/L -> L)
@R.SUBJECTPERSON.3@@R.SUBJECTNUMBER.PL@ìs3   #;
@R.SUBJECTPERSON.4@@R.SUBJECTNUMBER.SG@ìs3   #;

LEXICON sis-AFF-MiddlePrefix
@U.SUBJECTPERSON.1@@U.SUBJECTNUMBER.SG@^Psis  #; ! FLOATING L-TONE (M/L -> L)
! FIXME: No documentation of 2SG forms of sis-PFV verbs with /l/-initial stems.
@U.SUBJECTPERSON.2@@U.SUBJECTNUMBER.SG@^Psís  #; ! FLOATING L-TONE (M/L -> L)
@U.SUBJECTPERSON.3@@U.SUBJECTNUMBER.SG@ìs3   #;
@U.SUBJECTPERSON.1@@U.SUBJECTNUMBER.PL@@U.DIALECT.BIGPLUME@^Psaà  #; ! FLOATING L-TONE (M/L ->L)
@U.SUBJECTPERSON.1@@U.SUBJECTNUMBER.PL@@U.DIALECT.BIGCROW@^Psiì  #; ! FLOATING L-TONE (M/L -> L)
@U.SUBJECTPERSON.1@@U.SUBJECTNUMBER.PL@@U.DIALECT.CROWCHIEF@^Psiì  #; ! FLOATING L-TONE (M/L -L)
@U.SUBJECTPERSON.2@@U.SUBJECTNUMBER.PL@^Psas2 #; ! FLOATING L-TONE (M/L -> L)
@R.DIRECTOBJECTPRESENT.ON@@R.SUBJECTPERSON.3@@R.SUBJECTNUMBER.PL@^Ps3    #;
@R.DIRECTOBJECTPRESENT.ON@@R.SUBJECTPERSON.4@@R.SUBJECTNUMBER.SG@^Ps3    #;
@D.DIRECTOBJECTPRESENT@@R.SUBJECTPERSON.3@@R.SUBJECTNUMBER.PL@s3    #;
@D.DIRECTOBJECTPRESENT@@R.SUBJECTPERSON.4@@R.SUBJECTNUMBER.SG@s3    #;

LEXICON sis-AFF-OuterPrefix
@U.SUBJECTPERSON.1@@U.SUBJECTNUMBER.SG@sis   #;
! FIXME: No documentation of 2SG forms of sis-PFV verbs with /l/-initial stems.
@U.SUBJECTPERSON.2@@U.SUBJECTNUMBER.SG@sís   #;
@U.SUBJECTPERSON.3@@U.SUBJECTNUMBER.SG@sis3  #;
@U.SUBJECTPERSON.1@@U.SUBJECTNUMBER.PL@@U.DIALECT.BIGPLUME@saà  #;
@U.SUBJECTPERSON.1@@U.SUBJECTNUMBER.PL@@U.DIALECT.BIGCROW@siì   #;
@U.SUBJECTPERSON.1@@U.SUBJECTNUMBER.PL@@U.DIALECT.CROWCHIEF@siì #;
@U.SUBJECTPERSON.2@@U.SUBJECTNUMBER.PL@sas2  #;
@R.SUBJECTPERSON.3@@R.SUBJECTNUMBER.PL@s3    #;
@R.SUBJECTPERSON.4@@R.SUBJECTNUMBER.SG@s3    #;


LEXICON sii-AFF-NoPrefix
! FIXME: no documentation for this form; dummy values below
@U.SUBJECTPERSON.1@@U.SUBJECTNUMBER.SG@sis   #;
@U.SUBJECTPERSON.2@@U.SUBJECTNUMBER.SG@síí   #;
@U.SUBJECTPERSON.3@@U.SUBJECTNUMBER.SG@sii   #;	
@U.SUBJECTPERSON.1@@U.SUBJECTNUMBER.PL@@U.DIALECT.BIGPLUME@saà  #;
@U.SUBJECTPERSON.1@@U.SUBJECTNUMBER.PL@@U.DIALECT.BIGCROW@siì   #;
@U.SUBJECTPERSON.1@@U.SUBJECTNUMBER.PL@@U.DIALECT.CROWCHIEF@siì #;
@U.SUBJECTPERSON.2@@U.SUBJECTNUMBER.PL@sas2  #;
@R.SUBJECTPERSON.3@@R.SUBJECTNUMBER.PL@s3    #;	
@R.SUBJECTPERSON.4@@R.SUBJECTNUMBER.SG@s3    #;	

LEXICON sii-AFF-InnerPrefix
@U.SUBJECTPERSON.1@@U.SUBJECTNUMBER.SG@^Psis  #;
! When an inner prefix is present, 1SG forms also appear as Cas (e.g.,
! dìsists'ón ~ dasts'ón "I heard (it)", zìsisyín ~ zasyín "I killed (it)".
@U.SUBJECTPERSON.1@@U.SUBJECTNUMBER.SG@as    #;
@U.SUBJECTPERSON.2@@U.SUBJECTNUMBER.SG@^Psíí  #;
@U.SUBJECTPERSON.3@@U.SUBJECTNUMBER.SG@ìs3   #;
